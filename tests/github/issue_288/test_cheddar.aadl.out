       2     </priority>
     <text_memory_size>
       0     </text_memory_size>
     <stack_memory_size>
       0     </stack_memory_size>
     <period>
       50000     </period>
     <jitter>
       0     </jitter>
   </task>
 </tasks>
 <buffers/>
 <resources/>
 <dependencies/>
</cheddar>