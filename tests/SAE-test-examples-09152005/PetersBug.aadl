property set peter is
      SecurityLevel  : aadlinteger
      applies to (system, thread);
end peter;

data signal
end signal;

thread paul
features
	pe: in event port;
	pd: out data port signal;
properties
	-- 4.5 is not an integer
	peter::SecurityLevel => 4.5;
end paul;

thread implementation paul.default
end paul.default;

